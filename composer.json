{
    "name": "coresmart/laravel-gmail",
    "description": "Gmail API package for Laravel (Laravel 12 compatible fork)",
    "keywords": [
        "api",
        "laravel",
        "Gmail",
        "laravel-12"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Castro",
            "email": "danielcastro04@gmail.com",
            "homepage": "https://danielcastro.dev"
        },
        {
            "name": "Paul Burgess",
            "email": "paul@coresmart.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "google/apiclient": "^v2.15",
        "illuminate/auth": "^11.0|^12.0",
        "illuminate/config": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/session": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "symfony/mime": "^6.0|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CoreSmart\\LaravelGmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreSmart\\LaravelGmail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoreSmart\\LaravelGmail\\LaravelGmailServiceProvider"
            ],
            "aliases": {
                "LaravelGmail": "CoreSmart\\LaravelGmail\\Facade\\LaravelGmail"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
